#include <iostream>
using namespace std;

int main(){
    int a[5], b[5], c[10];
    int j = 0;

    cout << "Vector A:\n";
    for(int i = 0; i < 5; i++){
        cin >> a[i];
    }

    cout << "Vector B:\n";
    for(int i = 0; i < 5; i++){
        cin >> b[i];
    }

    for(int i = 0; i < 5; i++){
        c[j] = a[i];
        j++;
        c[j] = b[i];
        j++;
    }

    cout << "Intercalado:\n";
    for(int i = 0; i < 10; i++){
        cout << c[i] << " ";
    }

    return 0;
}

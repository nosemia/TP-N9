#include <iostream>
using namespace std;

int main(){
    int n;
    cout << "Cantidad: ";
    cin >> n;

    int v[n];

    for(int i = 0; i < n; i++){
        cout << "Valor " << i << ": ";
        cin >> v[i];
    }

    for(int i = 0; i < n; i++){
        for(int j = i+1; j < n; j++){
            if(v[j] < v[i]){
                int aux = v[i];
                v[i] = v[j];
                v[j] = aux;
            }
        }
    }

    cout << "Orden ascendente:\n";
    for(int i = 0; i < n; i++){
        cout << v[i] << " ";
    }

    return 0;
}

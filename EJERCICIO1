#include <iostream>
using namespace std;

int main(){
    int v[10];
    int pos = 0, neg = 0, cer = 0;

    for(int i = 0; i < 10; i++){
        cout << "Numero " << i << ": ";
        cin >> v[i];
    }

    for(int i = 0; i < 10; i++){
        if(v[i] > 0){
            pos++;
        }
        else if(v[i] < 0){
            neg++;
        }
        else{
            cer++;
        }
    }

    cout << "Positivos: " << pos << endl;
    cout << "Negativos: " << neg << endl;
    cout << "Ceros: " << cer;

    return 0;
}
